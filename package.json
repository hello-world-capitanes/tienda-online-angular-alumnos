{
  "name": "tienda-online-monica-rodriguez",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "serve": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --configuration development",
    "build:dev:stats": "npm run build:dev -- --stats-json",
    "build:prod": "ng build --configuration production --aot --delete-output-path --build-optimizer",
    "build:prod:stats": "npm run build:prod -- --stats-json",
    "analyze:build": "npx webpack-bundle-analyzer dist/tienda-online-angular-alumnos/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-covegare": "npm run test -- --code-coverage",
    "deploy": "firebase deploy --only hosting",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.3",
    "@angular/cdk": "^13.3.3",
    "@angular/common": "^13.3.3",
    "@angular/compiler": "^13.3.3",
    "@angular/core": "^13.3.3",
    "@angular/fire": "^7.3.0",
    "@angular/forms": "^13.2.6",
    "@angular/material": "^13.2.6",
    "@angular/platform-browser": "^13.2.6",
    "@angular/platform-browser-dynamic": "^13.2.6",
    "@angular/router": "^13.2.6",
    "firebase": "^9.6.11",
    "rxjs": "^7.5.5",
    "tslib": "^2.3.0",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.6",
    "@angular/cli": "^13.2.6",
    "@angular/compiler-cli": "^13.2.6",
    "@cypress/schematic": "^1.6.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.20.47",
    "jasmine-core": "~4.0.0",
    "karma": "^6.3.19",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2",
    "cypress": "latest",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
